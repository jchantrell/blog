{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  },
  "extends": "astro/tsconfigs/strict"
}
