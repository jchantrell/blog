---
import { PERSONAL_DATA } from '../content/data';
import WorkItem from './WorkItem.astro';

const { work } = PERSONAL_DATA;
---

<section class="flex min-h-0 flex-col gap-y-1">
  <h2 class="text-xl font-bold" id="work-experience">Experience</h2>
  {work.map((item) => (
            <WorkItem {...item}/>
    ))}
</section>
